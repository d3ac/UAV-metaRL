   "source": [
    "import torch\n",
    "\n",
    "# 假设 a 是一个包含三个形状为 (10, 7) 的张量的列表\n",
    "a = [torch.randn(10, 7), torch.randn(10, 7), torch.randn(10, 7)]\n",
    "\n",
    "# 使用 torch.cat() 连接张量\n",
    "concatenated_tensor = torch.cat(a, dim=0)\n",
    "\n",
    "# 打印新的张量形状\n",
    "print(\"新的张量形状：\", concatenated_tensor.shape)"
   ]
